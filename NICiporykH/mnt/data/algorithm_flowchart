// Algorithm Flowchart
digraph {
	rankdir=TB size=8
	A [label="Начало" shape=oval]
	B [label="Ввод N и M" shape=parallelogram]
	C [label="Инициализация матрицы N*M" shape=rectangle]
	D [label="Ввод элементов матрицы" shape=parallelogram]
	E [label="Отображение матрицы" shape=rectangle]
	F [label="Для каждой строки:
Подсчёт положительных элементов" shape=rectangle]
	G [label="Проверка:
Больше ли текущий счётчик максимального?" shape=diamond]
	H [label="Обновление максимального значения и номера строки" shape=rectangle]
	I [label="Вывод строки с наибольшим количеством положительных элементов" shape=parallelogram]
	J [label="Конец" shape=oval]
	A -> B
	B -> C
	C -> D
	D -> E
	E -> F
	F -> G
	G -> H [label="Да"]
	G -> F [label="Нет" constraint=false]
	H -> F
	F -> I [constraint=false]
	I -> J
}
